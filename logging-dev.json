{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
      "dev": {
          "format": "%(message)s"
    }
  },
  "handlers": {
    "console": {
        "class": "logging.StreamHandler",
        "stream": "ext://sys.stdout",
        "formatter": "dev"
    }
  },
  "loggers": {
  },
  "root": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": "no"
  }
}
